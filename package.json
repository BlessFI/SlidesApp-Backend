{
  "name": "slides-backend",
  "version": "1.0.0",
  "description": "Node.js backend with Fastify, TypeScript, Prisma, JWT auth",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed-reels": "npx tsx scripts/seed-reels.ts",
    "test": "npx tsx --test tests/multi-tenant.test.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/jwt": "^9.0.1",
    "@prisma/client": "^6.0.0",
    "bcrypt": "^5.1.1",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.10.1",
    "nodemon": "^3.1.7",
    "prisma": "^6.0.0",
"tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}

